    "platform": {
        "php": "^8.1"
    },
    "require": {
        "ext-curl": "*",
        "algolia/places": "*",
        "ckeditor/autogrow": "*",
        "ckeditor/codemirror": "*",
        "ckeditor/fakeobjects": "*",
        "ckeditor/image": "*",
        "ckeditor/link": "*",
        "codemirror/codemirror": "*",
        "composer/installers": "^1.9",
        "drupal/admin_toolbar": "^3.1",
        "drupal/antibot": "^2.0",
        "drupal/bootstrap5": "^1.1",
        "drupal/chosen": "^3.0",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/eu_cookie_compliance": "^1.19",
        "drupal/gin": "^3.0@alpha",
        "drupal/gin_login": "^1.0",
        "drupal/gutenberg": "^2.4",
        "drupal/honeypot": "^2.1",
        "drupal/mailsystem": "^4.3",
        "drupal/pathauto": "^1.9",
        "drupal/quick_node_clone": "^1.14",
        "drupal/restui": "^1.20",
        "drupal/scheduler": "^1.4",
        "drupal/smtp": "^1.0",
        "drupal/webform": "^6.1",
        "harvesthq/chosen": "^1.8",
        "jquery/inputmask": "*",
        "jquery/intl-tel-input": "*",
        "jquery/rateit": "*",
        "jquery/select2": "*",
        "jquery/textcounter": "*",
        "jquery/timepicker": "*",
        "progress-tracker/progress-tracker": "*",
        "signature_pad/signature_pad": "*",
        "tabby/tabby": "*",
        "tippyjs/6.x": "*",
        "vlucas/phpdotenv": "^5.4"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "autoload": {
        "files": [
            "load.environment.php"
        ]
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, you’ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "scripts": {
        "post-drupal-scaffold-cmd": [
            "patch -d ./web < ./patches/redirect_before_launch.patch",
            "cd web/themes/custom/ohano && npm install && npm run scss"
        ]
    },
    "require-dev": {
        "behat/mink-goutte-driver": "^2.0",
        "deployer/dist": "^6.8",
        "drupal/coder": "^8.3",
        "drush/drush": "^11.0",
        "phpspec/prophecy-phpunit": "^2.0",
        "phpunit/phpunit": "^9.5",
        "symfony/phpunit-bridge": "^6.0"
    }
}
